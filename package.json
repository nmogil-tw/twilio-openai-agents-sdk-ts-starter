{
  "dependencies": {
    "@docusaurus/core": "^3.8.1",
    "@docusaurus/module-type-aliases": "^3.8.1",
    "@docusaurus/preset-classic": "^3.8.1",
    "@docusaurus/tsconfig": "^3.8.1",
    "@docusaurus/types": "^3.8.1",
    "@openai/agents": "^0.0.10",
    "@types/node": "^24.0.12",
    "@types/uuid": "^10.0.0",
    "chalk": "^5.4.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "express": "^5.1.0",
    "express-ws": "^5.0.2",
    "fast-glob": "^3.3.3",
    "prism-react-renderer": "^2.4.1",
    "tsx": "^4.20.3",
    "twilio": "^4.21.0",
    "typescript": "^5.8.3",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "ws": "^8.18.3",
    "zod": "^3.25.76"
  },
  "name": "twilio-openai-agents-sdk-ts-starter",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "tsx examples/minimal/server.ts",
    "dev": "tsx watch examples/minimal/server.ts",
    "start-full": "tsx src/main.ts",
    "dev-full": "tsx watch src/main.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "echo \"Linting not configured yet\"",
    "typecheck": "tsc --noEmit",
    "cleanup": "tsx scripts/cleanup-sessions.ts",
    "example:test": "tsx examples/minimal/server.ts --test",
    "docs:serve": "npx docusaurus start website",
    "docs:build": "npx docusaurus build website",
    "docs:deploy": "npx docusaurus deploy website"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/express-ws": "^3.0.5",
    "@types/jest": "^29.5.12",
    "@types/supertest": "^6.0.2",
    "@types/ws": "^8.18.1",
    "jest": "^29.7.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/tests",
      "<rootDir>/src"
    ],
    "testMatch": [
      "**/__tests__/**/*.ts",
      "**/?(*.)+(spec|test).ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts",
      "!src/**/*.test.ts"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setup.ts"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.ts$": ["ts-jest", {
        "tsconfig": "<rootDir>/tests/tsconfig.json"
      }]
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(@openai/agents|@openai/agents-core)/)"
    ],
    "extensionsToTreatAsEsm": [".ts"],
    "moduleFileExtensions": ["ts", "js", "json"]
  }
}
