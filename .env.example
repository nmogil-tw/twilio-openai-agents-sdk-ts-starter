# Required for application to run
OPENAI_API_KEY=
# Logging level: debug, info, warn, error (default: info)
LOG_LEVEL=info
# Tracing (set to "false" to disable, default: true)
TRACING_ENABLED=true
# Name of the workflow/agent (default: Customer Service Agent)
WORKFLOW_NAME=Customer Service Agent
# Maximum number of conversation turns (default: 10)
MAX_TURNS=10
# Stream timeout in milliseconds (default: 30000)
STREAM_TIMEOUT_MS=30000
# Voice support is now integrated into the main server (no additional config needed)

TWILIO_ACCOUNT_SID=
TWILIO_API_KEY_SID=
TWILIO_API_KEY_SECRET=
TWILIO_PHONE_NUMBER=


# Model to use for the agent (e.g., gpt-4o, gpt-4o-mini, gpt-3.5-turbo)
AGENT_MODEL=gpt-4o-mini

# Segment Configuration
SEGMENT_WRITE_KEY=
SEGMENT_PROFILE_API_TOKEN=
SEGMENT_SPACE_ID=
SEGMENT_REGION=us
SUBJECT_RESOLVER=segment